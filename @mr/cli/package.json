{
  "name": "@mr/cli",
  "description": "Herramientas de linea de comandos de MR{FW}",
  "author": "José Antonio Jiménez",
  "scripts": {
    "mrlang": "yarn workspace @mr/cli node --no-warnings bin/mrlang",
    "mrpack": "yarn workspace @mr/cli node --no-warnings bin/mrpack",
    "compile": "yarn rspack --config src/rspack.config.mjs"
  },
  "bin": {
    "mrlang": "bin/mrlang.js",
    "mrpack": "bin/mrpack.js"
  },
  "config": {
    "bucket": "meteored-yarn-packages",
    "subible": true,
    "tipo": "root"
  },
  "main": "bin/mrpack.js",
  "exports": {
    "./manifest": "./manifest/workspace/index.ts",
    "./manifest/build": "./manifest/workspace/build/index.ts",
    "./manifest/build/bundle": "./manifest/workspace/build/bundle/index.ts",
    "./manifest/build/bundle/base": "./manifest/workspace/build/bundle/base.ts",
    "./manifest/build/bundle/componentes": "./manifest/workspace/build/bundle/componentes.ts",
    "./manifest/build/database": "./manifest/workspace/build/database.ts",
    "./manifest/deployment": "./manifest/workspace/deployment/index.ts",
    "./manifest/deployment/credenciales": "./manifest/workspace/deployment/credenciales.ts",
    "./manifest/deployment/imagen": "./manifest/workspace/deployment/imagen/index.ts",
    "./manifest/deployment/imagen/entorno": "./manifest/workspace/deployment/imagen/entorno.ts",
    "./manifest/deployment/kustomize": "./manifest/workspace/deployment/kustomize/index.ts",
    "./manifest/deployment/storage": "./manifest/workspace/deployment/storage/index.ts",
    "./manifest/deployment/storage/buckets": "./manifest/workspace/deployment/storage/buckets.ts",
    "./manifest/development": "./manifest/workspace/development.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@inquirer/prompts": "^7.10.1",
    "chokidar": "^4.0.3",
    "dd-trace": "^5.77.0",
    "dotenv": "^17.2.3",
    "jszip": "^3.10.1",
    "mysql2": "^3.15.3",
    "source-map-support": "^0.5.21",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@rspack/cli": "^1.6.3",
    "@rspack/core": "^1.6.3",
    "@tsconfig/node24": "^24.0.2",
    "@types/diff3": "^0.0.2",
    "@types/node": "^24.10.1",
    "@types/source-map-support": "^0.5.10",
    "css-loader": "^7.1.2",
    "diff3": "^0.0.4",
    "mini-css-extract-plugin": "^2.9.4",
    "pug": "^3.0.3",
    "pug3-loader": "^2.4.3",
    "rspack-manifest-plugin": "^5.1.0",
    "sass": "^1.94.0",
    "sass-loader": "^16.0.6",
    "services-comun": "workspace:*",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-checker-rspack-plugin": "^1.2.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "version": "2025.11.14+1-juancmartinez"
}
