{
    "enabled": true,
    "deploy": {
        "enabled": true,
        "type": "service",
        "runtime": "node",
        "target": "k8s",
        "alone": true,
        "arch": [
            "linux/amd64",
            "linux/arm64"
        ],
        "imagen": {
            "produccion": {
                "paquete": "services",
                "nombre": "logs-slave"
            },
            "test": {
                "paquete": "services",
                "nombre": "logs-slave"
            }
        },
        "kustomize": [
            {
                "name": "logs-slave",
                "dir": "services"
            }
        ]
    },
    "devel": {
        "enabled": true
    },
    "build": {
        "framework": "meteored",
        "database": {
            "produccion": "logs",
            "test": "logs"
        }
    }
}