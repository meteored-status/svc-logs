{
    "enabled": true,
    "deploy": {
        "enabled": true,
        "type": "service",
        "runtime": "node",
        "target": "lambda",
        "alone": true,
        "arch": [
            "linux/amd64",
            "linux/arm64"
        ],
        "credenciales": [
            {
                "source": "cloudrun-bigquery",
                "target": "bigquery.json"
            },
            {
                "source": "cloudrun-mysql-status-master-1",
                "target": "mysql.json"
            },
            {
                "source": "cloudrun-storage",
                "target": "storage.json"
            }
        ],
        "imagen": {
            "produccion": {
                "paquete": "services",
                "nombre": "logs-slave"
            },
            "test": {
                "paquete": "services",
                "nombre": "logs-slave"
            }
        },
        "kustomize": [
            {
                "name": "logs-slave",
                "dir": "services"
            }
        ],
        "cloudsql": [
            "meteored-status:europe-southwest1:status-master-1"
        ]
    },
    "devel": {
        "enabled": true
    },
    "build": {
        "framework": "meteored",
        "database": {
            "produccion": "logs",
            "test": "logs"
        }
    }
}